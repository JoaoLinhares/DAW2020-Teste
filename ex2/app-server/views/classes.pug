extends layout

block content
  h1 Classes Nível 1
  .w3-container.w3-margin
    table.w3-table-all
        tr
            th.w3-center Código
            th.w3-center Título
        each t in lista
            tr 
                td.w3-center
                    a(href='/classes/' + t.codigo) #{t.codigo}
                td.w3-center= t.titulo
    div.w3-container(style="text-align:center;")
            button(onclick="window.location.href='/'" style="text-align:center;") Home Page
    footer.w3-container.w3-teal
        address Generated By App-Server :: 2021-JAN, joaoLinhares